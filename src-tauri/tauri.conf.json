{
  "productName": "Hyz-tool",
  "version": "0.1.7",
  "identifier": "com.x.dev",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1431",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [

      {
        "title": "更新",
        "label": "update",
        "width": 400,
        "height": 200,
        "url": "/updater",
        "alwaysOnTop": true,
        "minimizable": false,

        "center": true,
        "visible": false
      },
      {
        "title": "退出系统",
        "label": "quit",
        "width": 300,
        "height": 150,
        "maxHeight": 400,
        "maxWidth": 200,
        "url": "/quit",
        "visible": false,
        "resizable": false,
        "devtools": false,
        "alwaysOnTop": true,
        "minimizable": false,
        "maximizable": false,
        "center": true
      },
      {
        "title": "ttt",
        "width": 1000,
        "height": 800,
        "url": "/"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgwRDgzQjEyOEY0QTk4QjcKUldTM21FcVBFanZZZ0R3ak5uYXZ1RUhmUWZhNitSb2IyY2J2TlJqY1FtbnFCWmVxMEtuMDRmdXYK",

      "endpoints": [
        "https://github.com/L1nY4n/ttt/releases/latest/download/latest.json"
      ]
    }
  }
}
