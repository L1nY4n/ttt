{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],

  "permissions": [
    "core:default",
    "shell:allow-open",
    "core:window:allow-minimize",
    "core:window:allow-destroy",
    "core:window:allow-set-title",
    "core:window:allow-hide",
    "core:window:allow-set-focus",
    "process:default"
  ]
}
